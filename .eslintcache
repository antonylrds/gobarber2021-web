ription }: Omit<ToastMessage, 'id'>) => {\n      const id = uuid();\n\n      const toast = {\n        id,\n        type,\n        title,\n        description,\n      };\n\n      setMessages(oldState => [...oldState, toast]);\n    },\n    [],\n  );\n\n  const removeToast = useCallback((id: string) => {\n    setMessages(oldState => oldState.filter(message => message.id !== id));\n  }, []);\n\n  return (\n    <ToastContext.Provider value={{ addToast, removeToast }}>\n      {children}\n      <ToastContainer messages={messages} />\n    </ToastContext.Provider>\n  );\n};\n\nfunction useToast(): ToastContextData {\n  const context = useContext(ToastContext);\n\n  if (!context) {\n    throw new Error('useToast must be used whitin a ToastProvider');\n  }\n\n  return context;\n}\n\nexport { ToastProvider, useToast };\n","C:\\Users\\Antony\\projects\\rocketseat\\gobarber2021-web\\src\\components\\ToastContainer\\Toast\\index.tsx",[],"C:\\Users\\Antony\\projects\\rocketseat\\gobarber2021-web\\src\\components\\ToastContainer\\Toast\\style.ts",[],"C:\\Users\\Antony\\projects\\rocketseat\\gobarber2021-web\\src\\routes\\index.tsx",[],"C:\\Users\\Antony\\projects\\rocketseat\\gobarber2021-web\\src\\pages\\Dashboard\\index.tsx",[],"C:\\Users\\Antony\\projects\\rocketseat\\gobarber2021-web\\src\\routes\\Route.tsx",[],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","severity":1,"message":"124","line":17,"column":10,"nodeType":"125","messageId":"126","endLine":17,"endColumn":40},{"ruleId":"123","severity":1,"message":"127","line":15,"column":12,"nodeType":"125","messageId":"126","endLine":15,"endColumn":45},{"ruleId":"123","severity":1,"message":"128","line":16,"column":15,"nodeType":"125","messageId":"126","endLine":16,"endColumn":25},"lines-around-directive",["129"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-pat